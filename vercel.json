{
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "functions": {
            "includeFiles": ["build.sh"]
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ]
  }
  